include "../common.dg"

include "extra/fqn_include1.dg"

module foo {
    include "extra/fqn_include1.dg"
    module bar {
        include "extra/fqn_include1.dg"
    }
}

extern fun main() c_int {
    println(modfqn_) // expect:

    println(baazle.modfqn_) // expect: baazle
    println(foozle.modfqn_) // expect: foozle
    println(foozle.baazle.modfqn_) // expect: foozle.baazle

    println(foo.modfqn_) // expect: foo
    println(foo.baazle.modfqn_) // expect: foo.baazle
    println(foo.foozle.modfqn_) // expect: foo.foozle
