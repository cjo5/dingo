include "../common.dg"

include "extra/fqn_include1.dg"

module foo {
    include "extra/fqn_include1.dg"
    module bar {
        include "extra/fqn_include1.dg"
    }
}

extern fun main() i32 {
    putln(__fqn__) // expect:

    putln(baazle.__fqn__) // expect: baazle
    putln(foozle.__fqn__) // expect: foozle
    putln(foozle.baazle.__fqn__) // expect: foozle.baazle

    putln(foo.__fqn__) // expect: foo
    putln(foo.baazle.__fqn__) // expect: foo.baazle
    putln(foo.foozle.__fqn__) // expect: foo.foozle
    putln(foo.foozle.baazle.__fqn__) // expect: foo.foozle.baazle
    
    putln(foo.bar.__fqn__) // expect: foo.bar
    putln(foo.bar.baazle.__fqn__) // expect: foo.bar.baazle
    putln(foo.bar.foozle.__fqn__) // expect: foo.bar.foozle
    putln(foo.bar.foozle.baazle.__fqn__) // expect: foo.bar.foozle.baazle

    return 0
}
