include "../common.dg"

include "extra/fqn_include1.dg"

module foo {
    include "extra/fqn_include1.dg"
    module bar {
        include "extra/fqn_include1.dg"
    }
}

extern fun main() c_int {
    println(__fqn__) // expect:

    println(baazle::__fqn__) // expect: baazle
    println(foozle::__fqn__) // expect: foozle
    println(foozle::baazle::__fqn__) // expect: foozle::baazle

    println(foo::__fqn__) // expect: foo
    println(foo::baazle::__fqn__) // expect: foo::baazle
    println(foo::foozle::__fqn__) // expect: foo::foozle
    println(foo::foozle::baazle::__fqn__) // expect: foo::foozle::baazle
    
    println(foo::bar::__fqn__) // expect: foo::bar
    println(foo::bar::baazle::__fqn__) // expect: foo::bar::baazle
    println(foo::bar::foozle::__fqn__) // expect: foo::bar::foozle
    println(foo::bar::foozle::baazle::__fqn__) // expect: foo::bar::foozle::baazle

    return 0
}
