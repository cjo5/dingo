include "../common.dg"

include "extra/scope_include1.dg"

fun print_fqn() {
    io::println("root")
}

fun print_scope_fqn() {
    ::print_fqn()
    up::print_fqn()
    print_fqn()
}

module foo {
    include "extra/scope_include1.dg"

    fun print_fqn() {
        ::io::println(__fqn__)
    }

    fun print_scope_fqn() {
        ::print_fqn()
        up::print_fqn()
        print_fqn()
    }

    module bar {
        include "extra/scope_include1.dg"

        fun print_fqn() {
            ::io::println(__fqn__)
        }

        fun print_scope_fqn() {
            ::print_fqn()
            up::print_fqn()
            print_fqn()
        }

        module car {
            fun print_up_up_fqn() {
                up::up::print_fqn()
            }
        } 
    }
}

extern fun main() C_int {
    print_scope_fqn()
    // expect: root
    // expect: root
    // expect: root

    baazle::print_scope_fqn()
    // expect: root
    // expect: root
    // expect: baazle

    foozle::print_scope_fqn()
    // expect: root
    // expect: root
    // expect: foozle

    foozle::baazle::print_scope_fqn()
    // expect: root
    // expect: foozle
    // expect: foozle::baazle

    foo::print_scope_fqn()
    // expect: root
    // expect: root
    // expect: foo

    foo::baazle::print_scope_fqn()
    // expect: root
    // expect: foo
    // expect: foo::baazle

    foo::foozle::print_scope_fqn()
    // expect: root
    // expect: foo
    // expect: foo::foozle

    foo::foozle::baazle::print_scope_fqn()
    // expect: root
    // expect: foo::foozle
    // expect: foo::foozle::baazle
    
    foo::bar::print_scope_fqn()
    // expect: root
    // expect: foo
    // expect: foo::bar

    foo::bar::baazle::print_scope_fqn()
    // expect: root
    // expect: foo::bar
    // expect: foo::bar::baazle

    foo::bar::foozle::print_scope_fqn()
    // expect: root
    // expect: foo::bar
    // expect: foo::bar::foozle

    foo::bar::foozle::baazle::print_scope_fqn()
    // expect: root
    // expect: foo::bar::foozle
    // expect: foo::bar::foozle::baazle

    foo::bar::car::print_up_up_fqn()
    // expect: foo

    return 0
}