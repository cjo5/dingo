include "../common.dg"

fun printFoo(f Foo) {
    puti(f.a)
    putchar(' ')
    puti(f.b)
    putchar(' ')
    puti(f.c)
    putchar('\n')
}

struct Foo {
    a i32
    b i32
    c i32
}

fun[c] main() c_int {
    var f Foo

    f = Foo{a: 1, b: 2, c: 3}
    printFoo(f) // expect-output: 1 2 3

    f = Foo{4, b: 5, c: 6} 
    printFoo(f) // expect-output: 4 5 6

    f = Foo{7, 8, c: 9}
    printFoo(f) // expect-output: 7 8 9 

    f = Foo{10, 11, 12}
    printFoo(f) // expect-output: 10 11 12

    f = Foo{c:3, a: 1, b: 2}
    printFoo(f) // expect-output: 1 2 3

    f = Foo{4, c: 6, b: 5} 
    printFoo(f) // expect-output: 4 5 6

    f = Foo{}
    printFoo(f) // expect-output: 0 0 0

    f = Foo{b: 2}
    printFoo(f) // expect-output: 0 2 0

    return 0
}