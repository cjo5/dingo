include "../common.dg"

extern fun main() c_int {
    var f1 = Foo{}
    var f2 = Foo{a: 9, b: Bar{c: 11, f: &var f1}}

    printFoo(&f2)
    // expect-output: 9
    // expect-output: 11

    f2.b.f.a = 13
    putiln(f1.a) // expect-output: 13

    return 0
}

fun printFoo(f &Foo) {
    putiln(f.a)
    putiln(f.b.c)
}

struct Foo {
    a i32
    b Bar
}

struct Bar {
    c i32
    f &var Foo
}
