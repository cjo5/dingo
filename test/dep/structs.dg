include "../common.dg"

extern fun main() C_int {
    var f1 = Foo()
    var f2 = Foo(a: 9, b: Bar(c: 11, f: &var f1))

    print_foo(&f2)
    // expect: 9
    // expect: 11

    f2.b.f.a = 13
    io::printiln(f1.a) // expect: 13

    return 0
}

fun print_foo(f: &Foo) {
    io::printiln(f.a)
    io::printiln(f.b.c)
}

struct Foo {
    var a: I32
    var b: Bar
}

struct Bar {
    var c: I32
    var f: &var Foo
}
