include "../common.dg"

extern fun main() c_int {
    var f1 = Foo()
    var f2 = Foo(a: 9, b: Bar(c: 11, f: &var f1))

    print_foo(&f2)
    // expect: 9
    // expect: 11

    f2.b.f.a = 13
    printiln(f1.a) // expect: 13

    return 0
}

fun print_foo(f: &Foo) {
    printiln(f.a)
    printiln(f.b.c)
}

struct Foo {
    a: i32
    b: Bar
}

struct Bar {
    c: i32
    f: &var Foo
}
