include "../common.dg"

fun[c] main() i32 {
    foo(2)
    // expect-exe: foo
    // expect-exe: bar
    // expect-exe: foo
    // expect-exe: bar
    
    return 0
}

fun foo(i i32) {
    if i < 0 {
        return
    }
    puts(c"foo")
    bar(i-1)
}

fun bar(i i32) {
    puts(c"bar")
    foo(i-1)
}