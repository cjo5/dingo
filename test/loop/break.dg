fun[c] main() i32 {
    for i = 1; i <= 5; i++ {
        for j = 1; j <= 5; j++ {
            putiln(i+j)
            if j == 2 {
                break
            }
        }
        if i == 3 {
            break
        }
    }

    // expect-exe: 2
    // expect-exe: 3
    // expect-exe: 3
    // expect-exe: 4
    // expect-exe: 4
    // expect-exe: 5
    
    var i = 5
    while i >= 1 {
        var j = 5
        while j >= 1 {
            putiln(i+j)
            if j == 2 {
                break
            }
            j--
        }
        if i == 4 {
            break
        }
        i--
    }

    // expect-exe: 10
