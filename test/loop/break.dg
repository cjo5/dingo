include "../common.dg"

extern fun main() c_int {
    for i = 1; i <= 5; i++ {
        for j = 1; j <= 5; j++ {
            putiln(i+j)
            if j == 2 {
                break
            }
        }
        if i == 3 {
            break
        }
    }

    // expect-output: 2
    // expect-output: 3
    // expect-output: 3
    // expect-output: 4
    // expect-output: 4
    // expect-output: 5
    
    var i = 5
    while i >= 1 {
        var j = 5
        while j >= 1 {
            putiln(i+j)
            if j == 2 {
                break
            }
            j--
        }
        if i == 4 {
            break
        }
        i--
    }

    // expect-output: 10
    // expect-output: 9
    // expect-output: 8
    // expect-output: 7
    // expect-output: 9
    // expect-output: 8
    // expect-output: 7
    // expect-output: 6
    
    return 0
}