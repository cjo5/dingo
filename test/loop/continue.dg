include "../common.dg"

fun[c] main() i32 {
    for i = 0; i < 5; i++ {
        if (i % 2) == 0 {
            continue
        }
        for j = 0; j < 5; j++ {
            if (j % 2) == 0 {
                continue
            }
            putiln(i+j)
        }
    }

    // expect-exe: 2
    // expect-exe: 4
    // expect-exe: 4
    // expect-exe: 6

    var i = 5 
    while i >= 1 {
        if (i % 2) == 0 {
            i--
            continue
        } 
        var j = 5
        while j >= 1 {
           if (j % 2) == 0 {
               j--
               continue
           } 
            putiln(i+j)
           j--
        }
       i--
    }

    // expect-exe: 10
    // expect-exe: 8
    // expect-exe: 6
    // expect-exe: 8
    // expect-exe: 6
    // expect-exe: 4
    // expect-exe: 6
    // expect-exe: 4
    // expect-exe: 2

    return 0
}