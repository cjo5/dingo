include "../common.dg"

fun foo(a i32, b i32, c i32) {
    puti(a)
    putchar(' ')
    puti(b)
    putchar(' ')
    puti(c)
    putchar('\n')
}

extern fun main() c_int {
    foo(a: 1, b: 2, c: 3) // expect-output: 1 2 3
    foo(4, b: 5, c: 6) // expect-output: 4 5 6
    foo(7, 8, c: 9) // expect-output: 7 8 9 
    foo(10, 11, 12) // expect-output: 10 11 12

    foo(c:3, a: 1, b: 2) // expect-output: 1 2 3
    foo(4, c: 6, b: 5) // expect-output: 4 5 6

    return 0
}