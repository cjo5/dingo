include "common.dg"

extern fun main() C_int {
    io::printbln(2 == 2) // expect: true
    io::printbln(1 == 2) // expect: false
    io::printbln(1 != 2) // expect: true
    io::printbln(1 != 1) // expect: false
    io::printbln(1 < 2) // expect: true
    io::printbln(2 < 1) // expect: false
    io::printbln(2 > 1) // expect: true
    io::printbln(1 > 2) // expect: false
    io::printbln(1 <= 2) // expect: true
    io::printbln(2 <= 2) // expect: true
    io::printbln(2 <= 1) // expect: false
    io::printbln(2 >= 1) // expect: true
    io::printbln(2 >= 2) // expect: true
    io::printbln(1 >= 2) // expect: false

    val two = 2
    val one = 1

    io::printbln(two == two) // expect: true
    io::printbln(one == two) // expect: false
    io::printbln(one != two) // expect: true
    io::printbln(one != one) // expect: false
    io::printbln(one < two) // expect: true
    io::printbln(two < one) // expect: false
    io::printbln(two > one) // expect: true
    io::printbln(one > two) // expect: false
    io::printbln(one <= two) // expect: true
    io::printbln(two <= two) // expect: true
    io::printbln(two <= one) // expect: false
    io::printbln(two >= one) // expect: true
    io::printbln(two >= two) // expect: true
    io::printbln(one >= two) // expect: false

    return 0
}