include "../common.dg"

extern fun main() c_int {
    const a1 = 15
    putiln(a1) // expect-output: 15

    const a2 = a1 + 3
    putiln(a2) // expect-output: 18

    const a3 = -a2
    putiln(a3) // expect-output: -18

    const b1 = true
    putbool(b1) // expect-output: true

    const b2 = b1 && false
    putbool(b2) // expect-output: false

    const b3 = !b2
    putbool(b3) // expect-output: true

    const c1 = Foo{a: 9}
    putiln(c1.a) // expect-output: 9

    const d1 = [1, 2, 3]
    putiln(d1[1]) // expect-output: 2

    return 0
}

struct Foo {
    a i32
}