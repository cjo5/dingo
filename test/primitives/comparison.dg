include "../common.dg"

fun[c] main() c_int {
    putbool(2 == 2) // expect-output: true
    putbool(1 == 2) // expect-output: false
    putbool(1 != 2) // expect-output: true
    putbool(1 != 1) // expect-output: false
    putbool(1 < 2) // expect-output: true
    putbool(2 < 1) // expect-output: false
    putbool(2 > 1) // expect-output: true
    putbool(1 > 2) // expect-output: false
    putbool(1 <= 2) // expect-output: true
    putbool(2 <= 2) // expect-output: true
    putbool(2 <= 1) // expect-output: false
    putbool(2 >= 1) // expect-output: true
    putbool(2 >= 2) // expect-output: true
    putbool(1 >= 2) // expect-output: false

    val two = 2
    val one = 1

    putbool(two == two) // expect-output: true
    putbool(one == two) // expect-output: false
    putbool(one != two) // expect-output: true
    putbool(one != one) // expect-output: false
    putbool(one < two) // expect-output: true
    putbool(two < one) // expect-output: false
    putbool(two > one) // expect-output: true
    putbool(one > two) // expect-output: false
    putbool(one <= two) // expect-output: true
    putbool(two <= two) // expect-output: true
    putbool(two <= one) // expect-output: false
    putbool(two >= one) // expect-output: true
    putbool(two >= two) // expect-output: true
    putbool(one >= two) // expect-output: false

    return 0
}