include "../common.dg"

fun[c] main() i32 {
    putiln(5+5) // expect-exe: 10
    putiln(7-5) // expect-exe: 2
    putiln(2-5) // expect-exe: -3
    putiln(5*5) // expect-exe: 25
    putiln(-5*5) // expect-exe: -25
    putiln(25/5) // expect-exe: 5
    putiln(-25/5) // expect-exe: -5
    putiln(5*2+1) // expect-exe: 11
    putiln(5*(2+1)) // expect-exe: 15
    putchar('a'+2) // expect-exe: c

    return 0
}