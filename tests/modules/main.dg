include "../common.dg"

import red

fun[c] main() {
    val f1 = red.Foo{a: 1}
    putiln(f1.a) // expect: 1
    putiln(red.bar) // expect: 10
    putiln(red.zoo()) // expect: 11

    val f2 = red.green.Foo{a: 2}
    putiln(f2.a) // expect: 2
    putiln(red.green.bar) // expect: 20
    putiln(red.green.zoo()) // expect: 21
}
