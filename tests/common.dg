// std
fun[c] atoi(_ *i8) i32
fun[c] exit(_ i32)
fun[c] abs(_ i32) i32

// stdio
fun[c] puts(_ *i8) i32
fun[c] putchar(_ i32) i32

// Recursively print each digit
fun puti(i i32) {
    val i2 = abs(i)
    if i2 < 10 {
        if i < 0 {
            putchar(45)
        }
    } else {
        puti(i/10)
    }
    putchar(48 + i2%10)
}