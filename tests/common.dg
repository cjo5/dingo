// std
fun[c] atoi(*i8) i32
fun[c] exit(i32)
fun[c] abs(i32) i32

// stdio
fun[c] puts(*i8) i32
fun[c] putchar(i32) i32

fun putiln(i i32) {
    puti(i)
    puts(c"")
}

// Recursively print each digit
fun puti(i i32) {
    val i2 = abs(i)
    if i2 < 10 {
        if i < 0 {
            putchar(45)
        }
    } else {
        puti(i/10)
    }
    putchar(48 + i2%10)
}