module simple;

include "libc.dg";
include "libstd.dg";

fun printVec3(v Vec3) {
    puti(v.v.x);
    puts("");
    puti(v.v.y);
    puts("");
    puti(v.z);
    puts("");
}

struct Vec2 {
    x i32;
    y i32;
}

struct Vec3 {
    v Vec2;
    z i32;
}

pub fun main() {
    var a = 5;
    var v Vec3 = Vec3{v=Vec2{x=a,y=9},z=13};
    printVec3(v);
}