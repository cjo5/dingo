func factorial(a: i32) -> i32 {
    if a == 0 {
        return 1;
    }
    return a * factorial(a-1);
}

func main() {
    val tmp: i32 = -5;
    println(tmp);
    val res: i32 = factorial(5);
    println(res);
}

