fun factorial(a) {
    if a == 0 {
        return 1;
    }
    return a * factorial(a-1);
}

fun main() {
    var i = 0;
    var sum = 0;

    while i < 5 {
        sum += i;

        if i == 2 {
            i += 2;
            continue;
        }

        i += 1;
    }

    println(sum);
}

